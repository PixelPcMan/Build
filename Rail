--new
Test1.CreateToggle("Drop", function(state)
    -- Si el estado es verdadero, activar la funci贸n
    if state then
        _G.on = true
        while _G.on do
            game:GetService("ReplicatedStorage").Remotes.DropItem:FireServer()
            wait()
        end
    else
        _G.on = false
    end
end)
