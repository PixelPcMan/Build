local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ZeianRussell/Kavo-UI-Library/main/Movable.source.lua"))()
local Window = Library.CreateLib("Dead Rail", "DarkTheme")
--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Maim")
--inicio
MainSection:NewButton("KillNpc", "Cam", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/PixelPcMan/Lubra/refs/heads/main/KillNpc"))()
end)
--new
MainSection:NewButton("Aimbot", "Head", function()
loadstring(game:HttpGet("https://pastebin.com/raw/NNRzb6Zr"))()
end)
--new
local Main = Window:NewTab("Tp")
local MainSection = Main:NewSection("Main")
--inicio
MainSection:NewButton("TpEnd", "Tp", function()
wait(1)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
character:WaitForChild("HumanoidRootPart")
local targetPosition = Vector3.new(-346, -69, -49060)

character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
end)
--new
MainSection:NewToggle("TpTren", "Tren", function()
    if state then
        _G.on = true
        while _G.on do
            local Players = game:GetService("Players")
local player = Players.LocalPlayer or Players:GetPlayerFromCharacter(script.Parent)

local vehicleSeat = workspace.Train.TrainControls.ConductorSeat.VehicleSeat

local function teleportToVehicleSeat()
    if player and player.Character then
        local character = player.Character
        -- Mueve al jugador a la posiciÃ³n del VehicleSeat
        character:SetPrimaryPartCFrame(vehicleSeat.CFrame)
    end
end

teleportToVehicleSeat()
            wait()
        end
    else
        _G.on = false
    end
end)
--new
MainSection:NewButton("TpTren", "Tp", function()
local Players = game:GetService("Players")
local player = Players.LocalPlayer or Players:GetPlayerFromCharacter(script.Parent)

local vehicleSeat = workspace.Train.TrainControls.ConductorSeat.VehicleSeat

local function teleportToVehicleSeat()
    if player and player.Character then
        local character = player.Character
        -- Mueve al jugador a la posiciÃ³n del VehicleSeat
        character:SetPrimaryPartCFrame(vehicleSeat.CFrame)
    end
end

teleportToVehicleSeat()
end)
--new
local Main = Window:NewTab("Drop")
local MainSection = Main:NewSection("Maim")
--inicio
MainSection:NewToggle("Drop", "Auto", function()
    if state then
        _G.on = true
        while _G.on do
            game:GetService("ReplicatedStorage").Remotes.DropItem:FireServer()
            wait()
        end
    else
        _G.on = false
    end
end)
--new
MainSection:NewButton("Drop", " ", function()
game:GetService("ReplicatedStorage").Remotes.DropItem:FireServer()
end)
--nee
MainSection:NewButton("Recojer", "Elimodo", function()
game:GetService("ReplicatedStorage").Remotes.StoreItem:FireServer(workspace.RuntimeItems.Coal)
game:GetService("ReplicatedStorage").Remotes.StoreItem:FireServer(workspace.RuntimeItems.GoldCup)
game:GetService("ReplicatedStorage").Remotes.StoreItem:FireServer(workspace.RuntimeItems.GoldPainting)
game:GetService("ReplicatedStorage").Remotes.StoreItem:FireServer(workspace.RuntimeItems.Crucifix)
end)
--Toggle
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ScreenGui"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false
local Toggle = Instance.new("TextButton")
Toggle.Name = "Toggle"
Toggle.Parent = ScreenGui
Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Toggle.Position = UDim2.new(0, 0, 0.454706937, 0)
Toggle.Size = UDim2.new(0, 50, 0,50)
Toggle.Font = Enum.Font.SourceSans
Toggle.Text = "ðŸ‘¾"
Toggle.TextColor3 = Color3.fromRGB(248, 248, 248)
Toggle.TextSize = 28.000
Toggle.Draggable = true
Toggle.MouseButton1Click:connect(function()
    Library:ToggleUI()
end)
local Corner = Instance.new("UICorner")
Corner.Name = "Corner"
Corner.Parent = Toggle
